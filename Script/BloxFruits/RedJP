Section:NewButton("auto trade bone", "ButtonInfo", function()
    print("Clicked")
end)
-- Script para Auto Trade de ossos em Blox Fruits

local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

-- Função para encontrar o NPC de troca de ossos
function findBoneTrader()
    for _, npc in pairs(workspace.NPCs:GetChildren()) do
        if npc.Name == "Death King" then
            return npc
        end
    end
    return nil
end

-- Função para realizar a troca de ossos
function tradeBones()
    local boneTrader = findBoneTrader()
    if boneTrader then
        humanoidRootPart.CFrame = boneTrader.HumanoidRootPart.CFrame
        wait(1) -- Espera 1 segundo para garantir a interação
        -- Código para interagir e trocar ossos
        -- Exemplo: Enviar evento remoto para realizar a troca
    end
end

-- Loop principal para Auto Trade de ossos
while true do
    tradeBones()
    wait(10) -- Espera 10 segundos antes de procurar novamente
end






