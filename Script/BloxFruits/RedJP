Section:NewButton("buy Iron Mace", "ButtonInfo", function()
    print("Clicked")
end)
local player = game.Players.LocalPlayer
local shop = game.Workspace:FindFirstChild("Sword Dealer of the West")

if shop then
    local purchaseEvent = shop:FindFirstChild("Purchase")
    if purchaseEvent then
        purchaseEvent:InvokeServer("Iron Mace")
        print("Iron Mace comprada com sucesso!")
    else
        print("Evento de compra não encontrado.")
    end
else
    print("Vendedor não encontrado.")
end








