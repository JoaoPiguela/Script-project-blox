Section:NewButton("Auto cake prince", "ButtonInfo", function()
    print("Clicked")
end)
-- Script para auto spawn do Cake Prince, derrotar NPCs e o Cake Prince na Cake Island

local function teleportToCakeIsland()
    -- Teleporta o jogador para a Cake Island
    local player = game.Players.LocalPlayer
    player.Character.HumanoidRootPart.CFrame = CFrame.new(0, 100, 0) -- Coordenadas da Cake Island
end

local function defeatNPCs()
    -- Derrota os NPCs especificados
    local npcs = {"Cookie Maker", "Cake Guardian", "Cook Employee", "Chief Baker"}
    for _, npcName in ipairs(npcs) do
        for _, npc in pairs(game.Workspace.NPCs:GetChildren()) do
            if npc.Name == npcName then
                while npc.Humanoid.Health > 0 do
                    player.Character.HumanoidRootPart.CFrame = npc.HumanoidRootPart.CFrame
                    wait(0.1)
                    game.ReplicatedStorage.Remotes.Combat:FireServer(npc)
                end
            end
        end
    end
end

local function teleportToCakePrince()
    -- Teleporta o jogador para o Cake Prince
    local cakePrince = game.Workspace:FindFirstChild("Cake Prince")
    if cakePrince then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = cakePrince.HumanoidRootPart.CFrame
    end
end

local function defeatCakePrince()
    -- Derrota o Cake Prince
    local cakePrince = game.Workspace:FindFirstChild("Cake Prince")
    if cakePrince then
        while cakePrince.Humanoid.Health > 0 do
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = cakePrince.HumanoidRootPart.CFrame
            wait(0.1)
            game.ReplicatedStorage.Remotes.Combat:FireServer(cakePrince)
        end
    end
end

local function autoSpawnCakePrince()
    -- Auto spawn do Cake Prince e teleporte
    while true do
        teleportToCakeIsland()
        defeatNPCs()
        teleportToCakePrince()
        defeatCakePrince()
        wait(10) -- Intervalo entre os spawns
    end
end

-- Inicia o script
autoSpawnCakePrince()





Section:NewButton("auto Mastery gun", "ButtonInfo", function()
    print("Clicked")
end)
-- Script para auto masterizar armas (guns) no Blox Fruits

local function teleportToNPC(npc)
    -- Teleporta o jogador para o NPC
    local player = game.Players.LocalPlayer
    player.Character.HumanoidRootPart.CFrame = npc.HumanoidRootPart.CFrame
end

local function defeatNPC(npc)
    -- Derrota o NPC
    local player = game.Players.LocalPlayer
    while npc.Humanoid.Health > 0 do
        player.Character.HumanoidRootPart.CFrame = npc.HumanoidRootPart.CFrame
        wait(0.1)
        game.ReplicatedStorage.Remotes.Combat:FireServer(npc)
    end
end

local function autoMasteryGun()
    -- Auto masterização de armas (guns)
    while true do
        for _, npc in pairs(game.Workspace.NPCs:GetChildren()) do
            if npc:FindFirstChild("Humanoid") and npc.Humanoid.Health > 0 then
                teleportToNPC(npc)
                defeatNPC(npc)
            end
        end
        wait(5) -- Intervalo entre as ações
    end
end

-- Inicia o script







