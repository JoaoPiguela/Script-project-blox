    for _, island in pairs(workspace:GetChildren()) do
        if island:IsA("Model") and island:FindFirstChild("IslandMarker") then
            createIslandESP(island)
        end
    end
end

-- Monitorar novas ilhas que aparecem no jogo
workspace.ChildAdded:Connect(function(child)
    if child:IsA("Model") and child:FindFirstChild("IslandMarker") then
        createIslandESP(child)
    end
end)

