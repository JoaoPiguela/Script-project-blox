Section:NewButton("Auto chest", "ButtonInfo", function()
    print("Clicked")
end)
-- Script de Auto Coletor de Baús para Blox Fruits

-- Função para coletar baús
function coletarBaus()
    -- Obter o jogador local
    local jogador = game.Players.LocalPlayer
    -- Obter a posição do jogador
    local posicaoJogador = jogador.Character.HumanoidRootPart.Position

    -- Loop para verificar todos os baús no jogo
    for _, baú in pairs(game.Workspace:GetChildren()) do
        if baú:IsA("Model") and baú.Name == "Chest" then
            -- Calcular a distância entre o jogador e o baú
            local distancia = (baú.PrimaryPart.Position - posicaoJogador).magnitude
            -- Se a distância for menor que um valor específico, coletar o baú
            if distancia < 50 then
                -- Mover o jogador para o baú
                jogador.Character.HumanoidRootPart.CFrame = baú.PrimaryPart.CFrame
                -- Esperar um pouco para garantir que o baú seja coletado
                wait(0.5)
            end
        end
    end
end

-- Loop infinito para coletar baús continuamente
while true do
    coletarBaus()
    -- Esperar um pouco antes de verificar novamente
    wait(5)
end




Section:NewButton("Auto farm al boss", "ButtonInfo", function()
    print("Clicked")
end)
-- Script de Auto Farm de Bosses para Blox Fruits (Mar 3)

-- Função para farmar o boss
function farmarBoss(bossName)
    local player = game.Players.LocalPlayer
    local boss = game.Workspace.Enemies:FindFirstChild(bossName)
    
    if boss then
        while boss and boss.Parent do
            player.Character.HumanoidRootPart.CFrame = boss.HumanoidRootPart.CFrame
            wait(0.5)
            -- Atacar o boss
            game:GetService("VirtualUser"):CaptureController()
            game:GetService("VirtualUser"):Button1Down(Vector2.new(0,0), workspace.CurrentCamera.CFrame)
            wait(0.1)
            game:GetService("VirtualUser"):Button1Up(Vector2.new(0,0), workspace.CurrentCamera.CFrame)
        end
    end
end

-- Lista de bosses para farmar no Mar 3
local bosses = {"Captain Elephant", "Stone", "Kilo Admiral", "Longma", "Island Empress" , "Beautiful Pirate", "Cake Queen"} -- Substitua pelos nomes reais dos bosses

-- Loop infinito para farmar todos os bosses
while true do
    for _, bossName in pairs(bosses) do
        farmarBoss(bossName)
        wait(5) -- Esperar um pouco antes de ir para o próximo boss
    end
end



local Tab = Window:NewTab("quest/itens")

Section:NewButton("Auto Cdk (Beta)", "ButtonInfo", function()
    print("Clicked")
end)

-- Script de Auto Missão para Cursed Dual Katana em Blox Fruits

-- Função para iniciar a missão da Cursed Dual Katana
local function startCDKMission()
    local player = game.Players.LocalPlayer
    local questGiver = game.Workspace.QuestGivers:FindFirstChild("Cursed Dual Katana Quest Giver")

    if questGiver then
        -- Simula a interação com o NPC para iniciar a missão
        game:GetService("ReplicatedStorage").Remotes.StartQuest:InvokeServer(questGiver)
        print("Missão da Cursed Dual Katana iniciada!")
    else
        print("NPC da missão não encontrado.")
    end
end

-- Função para coletar os pergaminhos
local function collectScrolls()
    local player = game.Players.LocalPlayer
    local scrollLocations = {
        game.Workspace.Scrolls.Scroll1,
        game.Workspace.Scrolls.Scroll2
    }

    for _, scroll in ipairs(scrollLocations) do
        if scroll then
            player.Character.HumanoidRootPart.CFrame = scroll.CFrame
            wait(1) -- Espera 1 segundo para simular a coleta
            fireclickdetector(scroll.ClickDetector)
            print("Pergaminho coletado: " .. scroll.Name)
        else
            print("Pergaminho não encontrado: " .. scroll.Name)
        end
    end
end

-- Função para derrotar o boss Cursed Skeleton
local function defeatCursedSkeleton()
    local player = game.Players.LocalPlayer
    local boss = game.Workspace.Bosses:FindFirstChild("Cursed Skeleton")

    if boss then
        player.Character.HumanoidRootPart.CFrame = boss.HumanoidRootPart.CFrame
        while boss.Humanoid.Health > 0 do
            -- Simula ataques ao boss
            game:GetService("ReplicatedStorage").Remotes.Attack:InvokeServer(boss)
            wait(0.5) -- Espera 0.5 segundos entre os ataques
        end
        print("Boss Cursed Skeleton derrotado!")
    else
        print("Boss Cursed Skeleton não encontrado.")
    end
end

-- Executa as funções de iniciar a missão, coletar os pergaminhos e derrotar o boss
startCDKMission()
wait(5) -- Espera 5 segundos antes de coletar os pergaminhos
collectScrolls()
wait(5) -- Espera 5 segundos antes de derrotar o boss
defeatCursedSkeleton()










Section:NewButton("Auto Soul guitar (Beta)", "ButtonInfo", function()
    print("Clicked")
end)
-- Script de Auto Soul Guitar na Lua Cheia para Blox Fruits

-- Função para verificar se é lua cheia
local function isFullMoon()
    local moon = game.Lighting:FindFirstChild("Moon")
    return moon and moon.Texture == "rbxassetid://1234567890" -- Substitua pelo ID da textura da lua cheia
end

-- Função para iniciar a missão da Soul Guitar
local function startSoulGuitarMission()
    local player = game.Players.LocalPlayer
    local questGiver = game.Workspace.QuestGivers:FindFirstChild("Soul Guitar Quest Giver")

    if questGiver then
        -- Simula a interação com o NPC para iniciar a missão
        game:GetService("ReplicatedStorage").Remotes.StartQuest:InvokeServer(questGiver)
        print("Missão da Soul Guitar iniciada!")
    else
        print("NPC da missão não encontrado.")
    end
end

-- Função para completar a missão da Soul Guitar
local function completeSoulGuitarMission()
    local player = game.Players.LocalPlayer
    local requiredItems = {"500 Bones", "250 Ectoplasm", "Dark Fragment", "5000 Fragments"}

    for _, item in ipairs(requiredItems) do
        -- Verifica se o jogador possui os itens necessários
        if player.Backpack:FindFirstChild(item) then
            print(item .. " encontrado.")
        else
            print(item .. " não encontrado.")
            return
        end
    end

    -- Simula a entrega dos itens para completar a missão
    game:GetService("ReplicatedStorage").Remotes.CompleteQuest:InvokeServer(unpack(requiredItems))
    print("Missão da Soul Guitar completada!")
end

-- Função principal para automatizar a obtenção da Soul Guitar
local function autoSoulGuitar()
    if isFullMoon() then
        startSoulGuitarMission()
        wait(5) -- Espera 5 segundos antes de tentar completar a missão
        completeSoulGuitarMission()
    else
        print("Não é lua cheia. Aguardando...")
    end
end

-- Executa a função principal
autoSoulGuitar()







Section:NewButton("Auto rip_indra, "ButtonInfo", function()
    print("Clicked")
end)
-- Script de Auto Rip_Indra para Blox Fruits

-- Função para encontrar e derrotar o boss Rip_Indra
local function defeatRipIndra()
    local player = game.Players.LocalPlayer
    local boss = game.Workspace.Bosses:FindFirstChild("Rip_Indra")

    if boss then
        -- Teleporta o jogador para o boss
        player.Character.HumanoidRootPart.CFrame = boss.HumanoidRootPart.CFrame
        wait(1) -- Espera 1 segundo para garantir que o jogador esteja no local

        -- Loop para atacar o boss até derrotá-lo
        while boss.Humanoid.Health > 0 do
            -- Simula ataques ao boss
            game:GetService("ReplicatedStorage").Remotes.Attack:InvokeServer(boss)
            wait(0.5) -- Espera 0.5 segundos entre os ataques
        end
        print("Boss Rip_Indra derrotado!")
    else
        print("Boss Rip_Indra não encontrado.")
    end
end

-- Executa a função para derrotar o boss Rip_Indra
defeatRipIndra()








Section:NewButton("Auto yama", "ButtonInfo", function()
    print("Clicked")
end)
-- Script de Auto Yama para Blox Fruits

-- Função para teletransportar para a Ilha Hydra
local function teleportToHydraIsland()
    local player = game.Players.LocalPlayer
    local hydraIslandLocation = CFrame.new(5227, 38, -1326) -- Coordenadas da Ilha Hydra
    player.Character.HumanoidRootPart.CFrame = hydraIslandLocation
    print("Teletransportado para a Ilha Hydra.")
end

-- Função para derrotar 30 Elite Bosses
local function defeatEliteBosses()
    local player = game.Players.LocalPlayer
    local eliteBossesDefeated = 0

    while eliteBossesDefeated < 30 do
        local boss = game.Workspace.Bosses:FindFirstChild("EliteBoss")
        if boss then
            player.Character.HumanoidRootPart.CFrame = boss.HumanoidRootPart.CFrame
            wait(1) -- Espera 1 segundo para garantir que o jogador esteja no local

            -- Loop para atacar o boss até derrotá-lo
            while boss.Humanoid.Health > 0 do
                game:GetService("ReplicatedStorage").Remotes.Attack:InvokeServer(boss)
                wait(0.5) -- Espera 0.5 segundos entre os ataques
            end
            eliteBossesDefeated = eliteBossesDefeated + 1
            print("Elite Boss derrotado! Total: " .. eliteBossesDefeated)
        else
            print("Elite Boss não encontrado. Tentando novamente em 10 segundos.")
            wait(10) -- Espera 10 segundos antes de procurar novamente
        end
    end
end

-- Função para verificar se o jogador é digno
local function checkWorthy()
    local player = game.Players.LocalPlayer
    local worthy = player:FindFirstChild("Worthy")

    if worthy and worthy.Value then
        print("Você é digno de obter a espada Yama.")
        return true
    else
        print("Você não é digno de obter a espada Yama.")
        return false
    end
end

-- Função principal para obter a espada Yama
local function getYamaSword()
    teleportToHydraIsland()
    wait(5) -- Espera 5 segundos antes de começar a derrotar os bosses
    defeatEliteBosses()
    wait(5) -- Espera 5 segundos antes de verificar se é digno

    if checkWorthy() then
        local yamaSwordLocation = CFrame.new(5240, 38, -1330) -- Coordenadas da espada Yama
        player.Character.HumanoidRootPart.CFrame = yamaSwordLocation
        print("Espada Yama obtida!")
    else
        print("Você não é digno de obter a espada Yama. Tente novamente.")
    end
end

-- Executa a função principal
getYamaSword()












