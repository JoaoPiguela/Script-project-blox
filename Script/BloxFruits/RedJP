local function buyBisento()
    local player = game.Players.LocalPlayer
    local shop = game:GetService("Workspace").Shops.BisentoShop

    if player and shop then
        local args = {
            [1] = "Purchase",
            [2] = "Bisento"
        }
        shop:InvokeServer(unpack(args))
        print("Bisento comprado com sucesso!")
    else
        print("Não foi possível encontrar o jogador ou a loja.")
    end
end

buyBisento()







