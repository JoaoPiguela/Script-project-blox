Section:NewButton("Auto farm level", "ButtonInfo", function()
    print("Clicked")
end)
-- Script para Auto Farm de nível em Blox Fruits

local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

-- Função para encontrar e aceitar quests
function acceptQuest()
    -- Código para encontrar o NPC de quest e aceitar a quest
    -- Exemplo: Procurar NPC, interagir e aceitar a quest
end

-- Função para farmar inimigos
function farmEnemies()
    for _, enemy in pairs(workspace.Enemies:GetChildren()) do
        if enemy:FindFirstChild("Humanoid") and enemy.Humanoid.Health > 0 then
            humanoidRootPart.CFrame = enemy.HumanoidRootPart.CFrame
            wait(0.5) -- Espera meio segundo para garantir o ataque
        end
    end
end

-- Loop principal para Auto Farm
while true do
    acceptQuest()
    farmEnemies()
    wait(1) -- Espera 1 segundo antes de repetir o loop
end


