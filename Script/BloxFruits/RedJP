local Tab = Window:NewTab("teleport")
Section:NewButton("tp Port Town", "ButtonInfo", function()
    print("Clicked")
end)
-- Script to teleport to Port Town in Blox Fruits
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()

-- Coordinates for Port Town
local portTownPosition = Vector3.new(-290, 43, 5530)

-- Function to teleport
local function teleportToPortTown()
    if character and character:FindFirstChild("HumanoidRootPart") then
        character.HumanoidRootPart.CFrame = CFrame.new(portTownPosition)
    else
        warn("Character or HumanoidRootPart not found!")
    end
end

-- Teleport the player
teleportToPortTown()




