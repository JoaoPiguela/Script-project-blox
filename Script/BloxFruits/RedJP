local Tab = Window:NewTab("Shop")

----------------Races---------------

Section:NewButton("buy Raçe Ghoul", "ButtonInfo", function()
    print("Clicked")
end)
-- Script to auto buy Ghoul race in Blox Fruits
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()

-- Function to buy Ghoul race
local function buyGhoulRace()
    -- Check if player has enough Ectoplasm and Hellfire Torch
    local ectoplasmCount = player.Backpack:FindFirstChild("Ectoplasm") and player.Backpack.Ectoplasm.Value or 0
    local hasHellfireTorch = player.Backpack:FindFirstChild("Hellfire Torch") ~= nil

    if ectoplasmCount >= 100 and hasHellfireTorch then
        -- Teleport to the NPC that sells Ghoul race
        local npcPosition = Vector3.new(1234, 56, 789) -- Replace with actual NPC coordinates
        character.HumanoidRootPart.CFrame = CFrame.new(npcPosition)

        -- Interact with the NPC to buy Ghoul race
        wait(2) -- Wait for teleport to complete
        local args = {
            [1] = "BuyRace",
            [2] = "Ghoul"
        }
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
    else
        warn("Not enough Ectoplasm or missing Hellfire Torch!")
    end
end

-- Execute the function
buyGhoulRace()








Section:NewButton("buy Raçe Cyborg", "ButtonInfo", function()
    print("Clicked")
end)
-- Script to auto buy Cyborg race in Blox Fruits
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()

-- Function to buy Cyborg race
local function buyCyborgRace()
    -- Check if player has the required items
    local hasFistOfDarkness = player.Backpack:FindFirstChild("Fist of Darkness") ~= nil
    local hasCoreBrain = player.Backpack:FindFirstChild("Core Brain") ~= nil

    if hasFistOfDarkness and hasCoreBrain then
        -- Teleport to the NPC that sells Cyborg race
        local npcPosition = Vector3.new(1234, 56, 789) -- Replace with actual NPC coordinates
        character.HumanoidRootPart.CFrame = CFrame.new(npcPosition)

        -- Interact with the NPC to buy Cyborg race
        wait(2) -- Wait for teleport to complete
        local args = {
            [1] = "BuyRace",
            [2] = "Cyborg"
        }
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
    else
        warn("Missing Fist of Darkness or Core Brain!")
    end
end

-- Execute the function
buyCyborgRace()


