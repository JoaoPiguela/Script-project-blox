-- Script de Auto Tushita para Blox Fruits

-- Função para teletransportar para a Ilha Hydra
local function teleportToHydraIsland()
    local player = game.Players.LocalPlayer
    local hydraIslandLocation = CFrame.new(5227, 38, -1326) -- Coordenadas da Ilha Hydra
    player.Character.HumanoidRootPart.CFrame = hydraIslandLocation
    print("Teletransportado para a Ilha Hydra.")
end

-- Função para ir atrás da cachoeira e quebrar a parede da sala Yama
local function breakYamaWall()
    local player = game.Players.LocalPlayer
    local yamaWallLocation = CFrame.new(5240, 38, -1330) -- Coordenadas da parede da sala Yama
    player.Character.HumanoidRootPart.CFrame = yamaWallLocation
    wait(1) -- Espera 1 segundo para garantir que o jogador esteja no local

    -- Simula a quebra da parede
    local yamaWall = game.Workspace:FindFirstChild("YamaWall")
    if yamaWall then
        fireclickdetector(yamaWall.ClickDetector)
        print("Parede da sala Yama quebrada.")
    else
        print("Parede da sala Yama não encontrada.")
    end
end

-- Função para se teletransportar para o portal atrás da água específica
local function teleportToPortal()
    local player = game.Players.LocalPlayer
    local portalLocation = CFrame.new(5300, 38, -1400) -- Coordenadas do portal
    player.Character.HumanoidRootPart.CFrame = portalLocation
    print("Teletransportado para o portal.")
end

-- Função para completar a missão com a tocha
local function completeTorchMission()
    local player = game.Players.LocalPlayer
    local torchLocation = CFrame.new(5350, 38, -1450) -- Coordenadas da tocha
    player.Character.HumanoidRootPart.CFrame = torchLocation
    print("Missão com a tocha iniciada.")
    
    -- Simula a interação com a tocha
    local torch = game.Workspace:FindFirstChild("Torch")
    if torch then
        fireclickdetector(torch.ClickDetector)
        print("Tocha acesa.")
    else
        print("Tocha não encontrada.")
    end
end

-- Função para derrotar o chefe Longma e obter a espada Tushita
local function defeatLongmaBoss()
    local player = game.Players.LocalPlayer
    local boss = game.Workspace.Bosses:FindFirstChild("Longma")

    if boss then
        player.Character.HumanoidRootPart.CFrame = boss.HumanoidRootPart.CFrame
        wait(1) -- Espera 1 segundo para garantir que o jogador esteja no local

        -- Loop para atacar o boss até derrotá-lo
        while boss.Humanoid.Health > 0 do
            -- Simula ataques ao boss
            game:GetService("ReplicatedStorage").Remotes.Attack:InvokeServer(boss)
            wait(0.5) -- Espera 0.5 segundos entre os ataques
        end
        print("Boss Longma derrotado!")
    else
        print("Boss Longma não encontrado.")
    end
end

-- Executa as funções passo a passo
teleportToHydraIsland()
wait(5) -- Espera 5 segundos antes de quebrar a parede
breakYamaWall()
wait(5) -- Espera 5 segundos antes de se teletransportar para o portal
teleportToPortal()
wait(5) -- Espera 5 segundos antes de completar a missão com a tocha
completeTorchMission()
wait(5) -- Espera 5 segundos antes de derrotar o  


Section:NewButton("Training Dummy", "ButtonInfo", function()
    print("Clicked")
end)
-- Script de Auto Training Dummy para Blox Fruits

-- Variáveis principais
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")
local questGiver = workspace:WaitForChild("QuestGiver") -- Certifique-se de que o nome do NPC está correto
local dummy = workspace:WaitForChild("TrainingDummy") -- Certifique-se de que o nome do dummy está correto

-- Função para pegar a quest
local function getQuest()
    humanoid:MoveTo(questGiver.Position)
    wait(1) -- Tempo de espera para chegar ao NPC
    fireproximityprompt(questGiver.ProximityPrompt) -- Interage com o NPC para pegar a quest
end

-- Função para atacar o dummy
local function autoTrain()
    while true do
        if dummy and dummy:FindFirstChild("Humanoid") then
            humanoid:MoveTo(dummy.Position)
            wait(1) -- Tempo de espera entre os ataques
            humanoid:MoveTo(dummy.Position)
        end
        wait(0.1) -- Intervalo entre as verificações
    end
end

-- Iniciar o processo de pegar a quest e treinar
getQuest()
autoTrain()





Section:NewButton("Auto buy Haki color", "ButtonInfo", function()
    print("Clicked")
end)
-- Script de Auto Buy Haki Color para Blox Fruits

-- Variáveis principais
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")
local hakiSeller = workspace:WaitForChild("master of auras") -- Certifique-se de que o nome do NPC está correto

-- Função para comprar Haki Color
local function buyHakiColor()
    humanoid:MoveTo(hakiSeller.Position)
    wait(1) -- Tempo de espera para chegar ao NPC
    fireproximityprompt(hakiSeller.ProximityPrompt) -- Interage com o NPC para abrir a loja
    wait(1) -- Tempo de espera para abrir a loja
    -- Supondo que o botão de compra tenha o nome "BuyButton"
    local buyButton = hakiSeller:FindFirstChild("BuyButton")
    if buyButton then
        fireclickdetector(buyButton.ClickDetector) -- Compra a cor de Haki
    end
end

-- Iniciar o processo de compra de Haki Color
buyHakiColor()


local Tab = Window:NewTab("Raid/Fruit")

Section:NewButton("auto raid", "ButtonInfo", function()
    print("Clicked")
end)
-- Script de Auto Raid para Blox Fruits

-- Variáveis principais
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")
local raidNPC = workspace:WaitForChild("RaidNPC") -- Certifique-se de que o nome do NPC está correto

-- Função para iniciar a raid
local function startRaid()
    humanoid:MoveTo(raidNPC.Position)
    wait(1) -- Tempo de espera para chegar ao NPC
    fireproximityprompt(raidNPC.ProximityPrompt) -- Interage com o NPC para iniciar a raid
end

-- Função para derrotar os NPCs durante a raid
local function defeatNPCs()
    while true do
        for _, npc in pairs(workspace:FindPartsInRegion3WithWhiteList(Region3.new(Vector3.new(-1000, 0, -1000), Vector3.new(1000, 1000, 1000)), {workspace.Enemies})) do
            if npc and npc:FindFirstChild("Humanoid") then
                humanoid:MoveTo(npc.Position)
                wait(1) -- Tempo de espera entre os ataques
                humanoid:MoveTo(npc.Position)
            end
        end
        wait(0.1) -- Intervalo entre as verificações
    end
end

-- Iniciar o processo de raid automática e derrotar NPCs
startRaid()
defeatNPCs()

Section:NewButton("Auto tp fruits", "ButtonInfo", function()
    print("Clicked")
end)
-- Script de Auto TP para Frutas em Blox Fruits

-- Variáveis principais
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

-- Função para teletransportar para a fruta mais próxima
local function teleportToFruit()
    while true do
        for _, fruit in pairs(workspace:GetChildren()) do
            if fruit:IsA("Tool") and fruit:FindFirstChild("Handle") then
                humanoidRootPart.CFrame = fruit.Handle.CFrame
                wait(1) -- Tempo de espera entre os teletransportes
            end
        end
        wait(5) -- Intervalo entre as verificações
    end
end

-- Iniciar o processo de teletransporte automático para frutas
teleportToFruit()




Section:NewButton("Auto store fruit, "ButtonInfo", function()
    print("Clicked")
end)
-- Script de Auto Store Fruit para Blox Fruits

-- Variáveis principais
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
local fruitStorageNPC = workspace:WaitForChild("FruitStorageNPC") -- Certifique-se de que o nome do NPC está correto

-- Função para armazenar a fruta
local function storeFruit()
    humanoidRootPart.CFrame = fruitStorageNPC.CFrame
    wait(1) -- Tempo de espera para chegar ao NPC
    fireproximityprompt(fruitStorageNPC.ProximityPrompt) -- Interage com o NPC para abrir o armazenamento
    wait(1) -- Tempo de espera para abrir o armazenamento
    -- Supondo que o botão de armazenamento tenha o nome "StoreButton"
    local storeButton = fruitStorageNPC:FindFirstChild("StoreButton")
    if storeButton then
        fireclickdetector(storeButton.ClickDetector) -- Armazena a fruta
    end
end

-- Função para detectar e armazenar frutas automaticamente
local function autoStoreFruit()
    while true do
        for _, fruit in pairs(workspace:GetChildren()) do
            if fruit:IsA("Tool") and fruit:FindFirstChild("Handle") then
                humanoidRootPart.CFrame = fruit.Handle.CFrame
                wait(1) -- Tempo de espera para pegar a fruta
                storeFruit() -- Armazena a fruta
            end
        end
        wait(5) -- Intervalo entre as verificações
    end
end

-- Iniciar o processo de armazenamento automático de frutas
autoStoreFruit()





Section:NewButton("Auto ramdonw fruit", "ButtonInfo", function()
    print("Clicked")
end)
-- Script de Auto Random Fruit para Blox Fruits

-- Variáveis principais
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
local fruitDealer = workspace:WaitForChild("FruitDealer") -- Certifique-se de que o nome do NPC está correto

-- Função para obter uma fruta aleatória
local function getRandomFruit()
    humanoidRootPart.CFrame = fruitDealer.CFrame
    wait(1) -- Tempo de espera para chegar ao NPC
    fireproximityprompt(fruitDealer.ProximityPrompt) -- Interage com o NPC para abrir a loja de frutas
    wait(1) -- Tempo de espera para abrir a loja
    -- Supondo que o botão de compra aleatória tenha o nome "RandomFruitButton"
    local randomFruitButton = fruitDealer:FindFirstChild("RandomFruitButton")
    if randomFruitButton then
        fireclickdetector(randomFruitButton.ClickDetector) -- Compra uma fruta aleatória
    end
end

-- Iniciar o processo de obtenção de frutas aleatórias
getRandomFruit()




Section:NewButton("Esp fruit", "ButtonInfo", function()
    print("Clicked")
end)
-- Script de ESP para Frutas em Blox Fruits

-- Função para criar um ESP para frutas
local function createFruitESP(fruit)
    local billboard = Instance.new("BillboardGui", fruit)
    billboard.Size = UDim2.new(0, 100, 0, 100)
    billboard.AlwaysOnTop = true

    local label = Instance.new("TextLabel", billboard)
    label.Size = UDim2.new(1, 0, 1, 0)
    label.BackgroundTransparency = 1
    label.Text = "Fruta"
    label.TextColor3 = Color3.fromRGB(255, 0, 0)
    label.TextScaled = true
end

-- Função para adicionar ESP a todas as frutas no jogo
local function addESPToFruits()
    for _, fruit in pairs(workspace:GetChildren()) do
        if fruit:IsA("Tool") and fruit:FindFirstChild("Handle") then
            createFruitESP(fruit)
        end
    end
end

-- Monitorar novas frutas que aparecem no jogo
workspace.ChildAdded:Connect(function(child)
    if child:IsA("Tool") and child:FindFirstChild("Handle") then
        createFruitESP(child)
    end
end)

-- Adicionar ESP às frutas já existentes
addESPToFruits()







Section:NewButton("esp player", "ButtonInfo", function()
    print("Clicked")
end)
-- Script de ESP para Jogadores em Blox Fruits

-- Função para criar um ESP para jogadores
local function createPlayerESP(player)
    if player ~= game.Players.LocalPlayer then
        local billboard = Instance.new("BillboardGui", player.Character.Head)
        billboard.Size = UDim2.new(0, 100, 0, 100)
        billboard.AlwaysOnTop = true

        local label = Instance.new("TextLabel", billboard)
        label.Size = UDim2.new(1, 0, 1, 0)
        label.BackgroundTransparency = 1
        label.Text = player.Name
        label.TextColor3 = Color3.fromRGB(0, 255, 0)
        label.TextScaled = true
    end
end

-- Função para adicionar ESP a todos os jogadores no jogo
local function addESPToPlayers()
    for _, player in pairs(game.Players:GetPlayers()) do
        if player.Character and player.Character:FindFirstChild("Head") then
            createPlayerESP(player)
        end
    end
end

-- Monitorar novos jogadores que entram no jogo
game.Players.PlayerAdded:Connect(function(player)
    player.CharacterAdded:Connect(function(character)
        if character:FindFirstChild("Head") then
            createPlayerESP(player)
        end
    end)
end)

-- Adicionar ESP aos jogadores já existentes
addESPToPlayers()








